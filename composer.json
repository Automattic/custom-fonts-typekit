{
  "name": "automattic/wpcomsh",
  "type": "project",
  "description": "A helper for connecting WordPress.com sites to external host infrastructure.",
  "license": "GPLv3",
  "require": {
    "automattic/at-pressable-podcasting": "v1.2.8",
    "automattic/custom-fonts": "v2.0.3",
    "automattic/custom-fonts-typekit": "1.0.8",
    "automattic/text-media-widget-styles": "~1.0",
    "automattic/wc-calypso-bridge": "1.9.12",
    "wordpress/classic-editor-plugin": "1.5",
    "automattic/jetpack-config": "^1.5",
    "automattic/jetpack-post-list": "^0.2.4"
  },
  "require-dev": {
    "php": ">=7.0.0"
  },
  "repositories": [
    {
      "type": "vcs",
      "no-api": true,
      "url": "https://github.com/Automattic/custom-fonts.git"
    },
    {
      "type": "vcs",
      "no-api": true,
      "url": "https://github.com/Automattic/custom-fonts-typekit.git"
    },
    {
      "type": "vcs",
      "no-api": true,
      "url": "https://github.com/automattic/at-pressable-podcasting.git"
    },
    {
      "type": "vcs",
      "no-api": true,
      "url": "https://github.com/Automattic/text-media-widget-styles.git"
    },
    {
      "type": "vcs",
      "no-api": true,
      "url": "https://github.com/Automattic/wc-calypso-bridge.git"
    },
    {
      "type": "package",
      "package": {
        "name": "wordpress/classic-editor-plugin",
        "version": "1.5",
        "dist": {
          "url": "https://downloads.wordpress.org/plugin/classic-editor.1.5.zip",
          "type": "zip"
        },
        "source": {
          "url": "https://plugins.svn.wordpress.org/classic-editor/",
          "type": "svn",
          "reference": "tags/1.5/"
        },
        "autoload": {
          "classmap": []
        }
      }
    }
  ],
  "config": {
    "allow-plugins": {
      "composer/installers": true
    }
  }
}
